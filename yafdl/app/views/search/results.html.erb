<h1>Search#results</h1>

<div id='results'>
  <table border="1">
    <tr>
      <th>Repository</th>
      <th>Title</th>
      <th>Autor(s)</th>
      <th>Subject(s)</th>
      <th>Identifier</th>
      <th>Document</th>
    </tr>
    <% @results.each do |result| %>
    <tr>
      <td><%= result.repository %></td>
      <td><%=result.title %></td>
      <td><%=result.creator %></td>
      <td><%=result.subject %></td>
      <td><%=result.identifier %></td>
      <td><%= link_to "descargar", result.url %></td>
    <tr/>
    <% end %>
  </table>
</div>
<br/>
<%= link_to "Volver", :action => :index %>
